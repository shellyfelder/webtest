{
  "version": 2,
  "builds": [
    {
      "src": "dist/my-website/browser/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run build -- --configuration=production",
  "outputDirectory": "dist/my-website/browser"
}