{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/my-website/browser"
      }
    }
  ],
  "buildCommand": "ng build --configuration=production --base-href=/",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/polyfills-.*\\.js$",
      "dest": "/polyfills-5CFQRCPP.js"
    },
    {
      "src": "/main-.*\\.js$",
      "dest": "/main-NZUM6XA7.js"
    },
    {
      "src": "/styles-.*\\.css$",
      "dest": "/styles-BG3ICWXW.css"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}